<% include ./partials/header %>

<div class="container">
                
    <form class="form-inline" action="/admin-panel/<%=car._id %>/?_method=PUT " method="POST">
        <div class="form-group">
             <label>Make</label>
             <input type="text" class="form-control" name="car[make]" value="<%=car.make%>">
        </div>

        <div class="form-group">
            <label>Model</label>
            <input type="text" class="form-control" name="car[model]" value="<%=car.model%>">
        </div>

        <div class="form-group">
             <label>Color</label>
             <input type="text" class="form-control" name="car[color]" value="<%=car.color%>">
        </div>

        <div class="form-group">
             <label>Year</label>
             <input type="text" class="form-control" name="car[year]" value="<%=car.year%>">
        </div>

        <div class="form-group">
             <label>Image</label>
             <input type="text" class="form-control" name="car[image]" value="<%=car.image%>">
        </div>

    <button type="submit" class="btn btn-default">Update</button>
    </form>
</div>




<% include ./partials/footer %>
