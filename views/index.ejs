<% include ./partials/header %>


<div class="container">
    <header class="jumbotron">
         <div class="container">
             <h2>Welcome To Awesome Car Rental!</h2>
             <p>Easy, affordable and sweet cars for everyone.</p>
             <p>
                <a class="btn btn-info btn-large" href="#">Rent A Car</a>
             </p>
         </div>

         <p>Pick Up Date: <input type="text" id="datepicker_test"></p>
         

    </header>

    
    


</div>

<div class="container">
    <div class="row text-center special-flex-mj">
        <% car.forEach(function(foundCar){ %>
            <div class="col-md-3 col-sm-6">
                <div class="thumbnail">
                   <img src="<%= foundCar.image %>">
                   <div class="caption">
                       <h5><%= foundCar.make %> <%= foundCar.model %></h5>
                       
                   </div>
                   <p>
                       <a href="#" class="btn btn-primary">Book Now</a>
                   </p>
                </div>
            </div>
        <% }); %>
    </div>
</div>

<% include ./partials/footer %>
